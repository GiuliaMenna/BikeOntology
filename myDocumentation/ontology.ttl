@prefix : <urn:webprotege:ontology:39025fa1-ef5a-42e5-9186-5ac2214979e6#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <urn:webprotege:ontology:39025fa1-ef5a-42e5-9186-5ac2214979e6#> .

<urn:webprotege:ontology:39025fa1-ef5a-42e5-9186-5ac2214979e6> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2002/07/owl#sameAs
owl:sameAs rdf:type owl:AnnotationProperty ;
           rdfs:label "owl:sameAs"@en .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/ontology/bikeo#comprises
<http://purl.org/ontology/bikeo#comprises> rdf:type owl:ObjectProperty ;
                                           rdfs:subPropertyOf owl:topObjectProperty ;
                                           rdfs:domain <http://purl.org/ontology/bikeo#CommonPartArea> ;
                                           rdfs:range <http://purl.org/ontology/bikeo#CommonPart> ;
                                           rdfs:comment "is a property defining which common part comprises a given piece."@en ;
                                           rdfs:label "comprises"@en .


###  http://purl.org/ontology/bikeo#hasBrand
<http://purl.org/ontology/bikeo#hasBrand> rdf:type owl:ObjectProperty ;
                                          rdfs:subPropertyOf owl:topObjectProperty ;
                                          rdfs:domain <http://purl.org/ontology/bikeo#Bike> ;
                                          rdfs:range <http://purl.org/ontology/bikeo#Brand> ;
                                          rdfs:comment "is a property defining which bike in particular has a given brand."@en ;
                                          rdfs:label "hasBrand"@en .


###  http://purl.org/ontology/bikeo#hasPart
<http://purl.org/ontology/bikeo#hasPart> rdf:type owl:ObjectProperty ;
                                         rdfs:subPropertyOf owl:topObjectProperty ;
                                         rdfs:domain <http://purl.org/ontology/bikeo#Bike> ;
                                         rdfs:range <http://purl.org/ontology/bikeo#CommonPart> ;
                                         rdfs:comment "is a property specifying how a bike is composed in its specific pieces."@en ;
                                         rdfs:label "hasPart"@en .


###  http://purl.org/ontology/bikeo#hasProblem
<http://purl.org/ontology/bikeo#hasProblem> rdf:type owl:ObjectProperty ;
                                            rdfs:subPropertyOf owl:topObjectProperty ;
                                            rdfs:domain <http://purl.org/ontology/bikeo#CommonPart> ;
                                            rdfs:range <http://purl.org/ontology/bikeo#Problem> ;
                                            rdfs:comment "is a property defining a problem that could affect a bike or a piece of a bike."@en ;
                                            rdfs:label "hasProblem"@en .


###  http://purl.org/ontology/bikeo#hasSolution
<http://purl.org/ontology/bikeo#hasSolution> rdf:type owl:ObjectProperty ;
                                             rdfs:subPropertyOf owl:topObjectProperty ;
                                             rdfs:domain <http://purl.org/ontology/bikeo#Problem> ;
                                             rdfs:range <http://purl.org/ontology/bikeo#Solution> ;
                                             rdfs:comment "is the property telling how a problem of a bike can be solved and through which tools."@en ;
                                             rdfs:label "hasSolution" .


###  http://purl.org/ontology/bikeo#hasType
<http://purl.org/ontology/bikeo#hasType> rdf:type owl:ObjectProperty ;
                                         rdfs:subPropertyOf owl:topObjectProperty ;
                                         rdfs:domain <http://purl.org/ontology/bikeo#Bike> ;
                                         rdfs:range <http://purl.org/ontology/bikeo#TypeOfBike> ;
                                         rdfs:comment "is a property that defines the specific type of bike of a bike model."@en ;
                                         rdfs:label "hasType"@en .


###  http://purl.org/ontology/bikeo#isBrandOf
<http://purl.org/ontology/bikeo#isBrandOf> rdf:type owl:ObjectProperty ;
                                           rdfs:subPropertyOf owl:topObjectProperty ;
                                           rdfs:domain <http://purl.org/ontology/bikeo#Brand> ;
                                           rdfs:range <http://purl.org/ontology/bikeo#Bike> ;
                                           rdfs:comment "is a property used for specifying which specific brand owns and produces a given bike model."@en ;
                                           rdfs:label "isBrandOf"@en .


###  http://purl.org/ontology/bikeo#isComprisedIn
<http://purl.org/ontology/bikeo#isComprisedIn> rdf:type owl:ObjectProperty ;
                                               rdfs:subPropertyOf owl:topObjectProperty ;
                                               rdfs:domain <http://purl.org/ontology/bikeo#CommonPart> ;
                                               rdfs:range <http://purl.org/ontology/bikeo#CommonPartArea> ;
                                               rdfs:comment "is a property defining which piece is comprised in a given common part."@en ;
                                               rdfs:label "isComprisedIn"@en .


###  http://purl.org/ontology/bikeo#isPartOf
<http://purl.org/ontology/bikeo#isPartOf> rdf:type owl:ObjectProperty ;
                                          rdfs:subPropertyOf owl:topObjectProperty ;
                                          rdfs:domain <http://purl.org/ontology/bikeo#CommonPart> ;
                                          rdfs:range <http://purl.org/ontology/bikeo#Bike> ;
                                          rdfs:comment "is a property used for specifying which common parts are included in a bike."@en ;
                                          rdfs:label "isPartOf"@en .


###  http://purl.org/ontology/bikeo#isProblemOf
<http://purl.org/ontology/bikeo#isProblemOf> rdf:type owl:ObjectProperty ;
                                             rdfs:subPropertyOf owl:topObjectProperty ;
                                             rdfs:domain <http://purl.org/ontology/bikeo#Problem> ;
                                             rdfs:range <http://purl.org/ontology/bikeo#CommonPart> ;
                                             rdfs:comment "is a property defining to which piece is connected a given problem."@en ;
                                             rdfs:label "isProblemOf"@en .


###  http://purl.org/ontology/bikeo#isRequestedFor
<http://purl.org/ontology/bikeo#isRequestedFor> rdf:type owl:ObjectProperty ;
                                                rdfs:subPropertyOf owl:topObjectProperty ;
                                                rdfs:domain <http://purl.org/ontology/bikeo#Tool> ;
                                                rdfs:range <http://purl.org/ontology/bikeo#Solution> ;
                                                rdfs:comment "is a property defining which tool is required in order to obtain a given solution to a problem."@en ;
                                                rdfs:label "isRequestedFor"@en .


###  http://purl.org/ontology/bikeo#isSolutionOf
<http://purl.org/ontology/bikeo#isSolutionOf> rdf:type owl:ObjectProperty ;
                                              rdfs:subPropertyOf owl:topObjectProperty ;
                                              rdfs:domain <http://purl.org/ontology/bikeo#Solution> ;
                                              rdfs:range <http://purl.org/ontology/bikeo#Problem> ;
                                              rdfs:comment "is a property defining to which piece is connected a given solution of a problem."@en ;
                                              rdfs:label "isSolutionOf"@en .


###  http://purl.org/ontology/bikeo#isTypeOf
<http://purl.org/ontology/bikeo#isTypeOf> rdf:type owl:ObjectProperty ;
                                          rdfs:subPropertyOf owl:topObjectProperty ;
                                          rdfs:domain <http://purl.org/ontology/bikeo#TypeOfBike> ;
                                          rdfs:range <http://purl.org/ontology/bikeo#Bike> ;
                                          rdfs:comment "this property defines that a specific bike model is a type of bike."@en ;
                                          rdfs:label "isTypeOf"@en .


###  http://purl.org/ontology/bikeo#producedBy
<http://purl.org/ontology/bikeo#producedBy> rdf:type owl:ObjectProperty ;
                                            rdfs:subPropertyOf owl:topObjectProperty ;
                                            rdfs:domain <http://purl.org/ontology/bikeo#CommonPart> ;
                                            rdfs:range <http://purl.org/ontology/bikeo#Brand> ;
                                            rdfs:comment "defines the specific part of a bike that is produced by a given brand."@en ;
                                            rdfs:label "producedBy"@en .


###  http://purl.org/ontology/bikeo#produces
<http://purl.org/ontology/bikeo#produces> rdf:type owl:ObjectProperty ;
                                          rdfs:subPropertyOf owl:topObjectProperty ;
                                          rdfs:domain <http://purl.org/ontology/bikeo#Brand> ;
                                          rdfs:range <http://purl.org/ontology/bikeo#CommonPart> ;
                                          rdfs:comment "is a property defining which brand produces a specific part of a bike."@en ;
                                          rdfs:label "produces"@en .


###  http://purl.org/ontology/bikeo#requests
<http://purl.org/ontology/bikeo#requests> rdf:type owl:ObjectProperty ;
                                          rdfs:subPropertyOf owl:topObjectProperty ;
                                          rdfs:domain <http://purl.org/ontology/bikeo#Solution> ;
                                          rdfs:range <http://purl.org/ontology/bikeo#Tool> ;
                                          rdfs:comment "is a property defining which solution to a problem requests a given tool to be used."@en ;
                                          rdfs:label "requests"@en .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/ontology/bikeo#hasChainRing
<http://purl.org/ontology/bikeo#hasChainRing> rdf:type owl:DatatypeProperty ;
                                              rdfs:domain <http://purl.org/ontology/bikeo#Crank> ;
                                              rdfs:range xsd:string ;
                                              rdfs:label "hasChainRing"@en .


###  http://purl.org/ontology/bikeo#hasGearing
<http://purl.org/ontology/bikeo#hasGearing> rdf:type owl:DatatypeProperty ;
                                            rdfs:domain <http://purl.org/ontology/bikeo#Bike> ;
                                            rdfs:range xsd:string ;
                                            rdfs:label "hasGearing"@en .


###  http://purl.org/ontology/bikeo#hasSize
<http://purl.org/ontology/bikeo#hasSize> rdf:type owl:DatatypeProperty ;
                                         rdfs:subPropertyOf owl:topDataProperty ;
                                         rdfs:domain <http://purl.org/ontology/bikeo#Tire> ;
                                         rdfs:range xsd:string ;
                                         rdfs:label "hasSize"@en .


#################################################################
#    Classes
#################################################################

###  http://purl.org/ontology/bikeo#AdjustSeatpost
<http://purl.org/ontology/bikeo#AdjustSeatpost> rdf:type owl:Class ;
                                                rdfs:subClassOf <http://purl.org/ontology/bikeo#Solution> ,
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty <http://purl.org/ontology/bikeo#isSolutionOf> ;
                                                                  owl:someValuesFrom <http://purl.org/ontology/bikeo#SeatHeight>
                                                                ] ,
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty <http://purl.org/ontology/bikeo#requests> ;
                                                                  owl:someValuesFrom <http://purl.org/ontology/bikeo#AllenKey>
                                                                ] ;
                                                rdfs:comment "the possible solution to a wrong seat height that can be changed through the use of the allen key."@en ;
                                                rdfs:label "AdjustSeatpost"@en .


###  http://purl.org/ontology/bikeo#AllenKey
<http://purl.org/ontology/bikeo#AllenKey> rdf:type owl:Class ;
                                          rdfs:subClassOf <http://purl.org/ontology/bikeo#Tool> ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty <http://purl.org/ontology/bikeo#isRequestedFor> ;
                                                            owl:someValuesFrom <http://purl.org/ontology/bikeo#AdjustSeatpost>
                                                          ] ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty <http://purl.org/ontology/bikeo#isRequestedFor> ;
                                                            owl:someValuesFrom <http://purl.org/ontology/bikeo#Tighten>
                                                          ] ;
                                          rdfs:comment "a simple driver for bolts or screws that have heads with internal hexagonal recesses (sockets)."@en ;
                                          rdfs:label "AllenKey"@en .


###  http://purl.org/ontology/bikeo#Battery
<http://purl.org/ontology/bikeo#Battery> rdf:type owl:Class ;
                                         rdfs:subClassOf <http://purl.org/ontology/bikeo#CommonPart> ;
                                         rdfs:comment "rechargeable batteries used by E-bikes in addition to electric motors and some form of control."@en ;
                                         rdfs:label "Battery"@en .


###  http://purl.org/ontology/bikeo#Bike
<http://purl.org/ontology/bikeo#Bike> rdf:type owl:Class ;
                                      rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                        owl:onProperty <http://purl.org/ontology/bikeo#hasBrand> ;
                                                        owl:someValuesFrom <http://purl.org/ontology/bikeo#Brand>
                                                      ] ,
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty <http://purl.org/ontology/bikeo#hasPart> ;
                                                        owl:someValuesFrom <http://purl.org/ontology/bikeo#CommonPart>
                                                      ] ,
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty <http://purl.org/ontology/bikeo#hasType> ;
                                                        owl:someValuesFrom <http://purl.org/ontology/bikeo#TypeOfBike>
                                                      ] ,
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty <http://purl.org/ontology/bikeo#hasGearing> ;
                                                        owl:someValuesFrom xsd:string
                                                      ] ;
                                      rdfs:comment "A bike or bicycle is a two-wheeled steerable machine that is pedaled by the rider’s feet."@en ;
                                      rdfs:label "Bike"@en ;
                                      owl:sameAs "vso:Bicycle"@en .


###  http://purl.org/ontology/bikeo#Brakes
<http://purl.org/ontology/bikeo#Brakes> rdf:type owl:Class ;
                                        rdfs:subClassOf <http://purl.org/ontology/bikeo#CommonPart> ,
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty <http://purl.org/ontology/bikeo#hasProblem> ;
                                                          owl:someValuesFrom <http://purl.org/ontology/bikeo#WornBrakes>
                                                        ] ;
                                        rdfs:comment "devices acting on the wheel used to stop or slow down a bicycle."@en ;
                                        rdfs:label "Brakes"@en .


###  http://purl.org/ontology/bikeo#Brand
<http://purl.org/ontology/bikeo#Brand> rdf:type owl:Class ;
                                       rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                         owl:onProperty <http://purl.org/ontology/bikeo#isBrandOf> ;
                                                         owl:someValuesFrom <http://purl.org/ontology/bikeo#Bike>
                                                       ] ,
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty <http://purl.org/ontology/bikeo#produces> ;
                                                         owl:someValuesFrom <http://purl.org/ontology/bikeo#CommonPart>
                                                       ] ;
                                       rdfs:comment "a company or organization which has produced the bike or its parts."@en ;
                                       rdfs:label "Brand"@en .


###  http://purl.org/ontology/bikeo#BrokenPart
<http://purl.org/ontology/bikeo#BrokenPart> rdf:type owl:Class ;
                                            rdfs:subClassOf <http://purl.org/ontology/bikeo#Problem> ,
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <http://purl.org/ontology/bikeo#hasSolution> ;
                                                              owl:someValuesFrom <http://purl.org/ontology/bikeo#ReplacePart>
                                                            ] ,
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <http://purl.org/ontology/bikeo#isProblemOf> ;
                                                              owl:someValuesFrom <http://purl.org/ontology/bikeo#CommonPart>
                                                            ] ;
                                            rdfs:comment "all the parts of a bike in need of some adjustments."@en ;
                                            rdfs:label "BrokenPart"@en .


###  http://purl.org/ontology/bikeo#Chain
<http://purl.org/ontology/bikeo#Chain> rdf:type owl:Class ;
                                       rdfs:subClassOf <http://purl.org/ontology/bikeo#CommonPart> ,
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty <http://purl.org/ontology/bikeo#isComprisedIn> ;
                                                         owl:someValuesFrom <http://purl.org/ontology/bikeo#ChainArea>
                                                       ] ;
                                       rdfs:comment "a series of (usually metal) rings or links fitted into one another to make a flexible ligament."@en ;
                                       rdfs:label "Chain"@en .


###  http://purl.org/ontology/bikeo#ChainArea
<http://purl.org/ontology/bikeo#ChainArea> rdf:type owl:Class ;
                                           rdfs:subClassOf <http://purl.org/ontology/bikeo#CommonPartArea> ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://purl.org/ontology/bikeo#comprises> ;
                                                             owl:someValuesFrom <http://purl.org/ontology/bikeo#Chain>
                                                           ] ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://purl.org/ontology/bikeo#comprises> ;
                                                             owl:someValuesFrom <http://purl.org/ontology/bikeo#FrontDerailleur>
                                                           ] ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://purl.org/ontology/bikeo#hasProblem> ;
                                                             owl:someValuesFrom <http://purl.org/ontology/bikeo#SnappedChain>
                                                           ] ;
                                           rdfs:comment "the structural part of the bike, fitted together with the frame and the wheels, that includes the chain and the front derailleur." ;
                                           rdfs:label "ChainArea"@en .


###  http://purl.org/ontology/bikeo#ChainLink
<http://purl.org/ontology/bikeo#ChainLink> rdf:type owl:Class ;
                                           rdfs:subClassOf <http://purl.org/ontology/bikeo#Tool> ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://purl.org/ontology/bikeo#isRequestedFor> ;
                                                             owl:someValuesFrom <http://purl.org/ontology/bikeo#RefitChain>
                                                           ] ;
                                           rdfs:comment "a set of inner chain plates joined to a set of outer chain plates by a rivet or pin."@en ;
                                           rdfs:label "ChainLink"@en .


###  http://purl.org/ontology/bikeo#ChainRivet
<http://purl.org/ontology/bikeo#ChainRivet> rdf:type owl:Class ;
                                            rdfs:subClassOf <http://purl.org/ontology/bikeo#Tool> ,
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <http://purl.org/ontology/bikeo#isRequestedFor> ;
                                                              owl:someValuesFrom <http://purl.org/ontology/bikeo#RefitChain>
                                                            ] ;
                                            rdfs:comment "a mechanical fastener composed of a head on one end and a cylindrical stem on another (called the tail) which has the appearance of a metal pin."@en ;
                                            rdfs:label "ChainRivet"@en .


###  http://purl.org/ontology/bikeo#ChainStay
<http://purl.org/ontology/bikeo#ChainStay> rdf:type owl:Class ;
                                           rdfs:subClassOf <http://purl.org/ontology/bikeo#CommonPart> ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://purl.org/ontology/bikeo#isComprisedIn> ;
                                                             owl:someValuesFrom <http://purl.org/ontology/bikeo#Frame>
                                                           ] ;
                                           rdfs:comment "the part of the frame tube that run parallel to the chain, connecting the bottom bracket shell (which holds the axis around which the pedals and cranks rotate) to the rear fork ends or dropouts."@en ;
                                           rdfs:label "ChainStay"@en .


###  http://purl.org/ontology/bikeo#Cogset
<http://purl.org/ontology/bikeo#Cogset> rdf:type owl:Class ;
                                        rdfs:subClassOf <http://purl.org/ontology/bikeo#CommonPart> ;
                                        rdfs:comment "the set of rear sprockets that attaches to the hub on the rear wheel."@en ;
                                        rdfs:label "Cogset"@en .


###  http://purl.org/ontology/bikeo#CommonPart
<http://purl.org/ontology/bikeo#CommonPart> rdf:type owl:Class ;
                                            rdfs:subClassOf owl:Thing ,
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <http://purl.org/ontology/bikeo#hasProblem> ;
                                                              owl:someValuesFrom <http://purl.org/ontology/bikeo#CommonPart>
                                                            ] ,
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <http://purl.org/ontology/bikeo#hasProblem> ;
                                                              owl:someValuesFrom <http://purl.org/ontology/bikeo#Problem>
                                                            ] ,
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <http://purl.org/ontology/bikeo#isPartOf> ;
                                                              owl:someValuesFrom <http://purl.org/ontology/bikeo#Bike>
                                                            ] ,
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <http://purl.org/ontology/bikeo#producedBy> ;
                                                              owl:someValuesFrom <http://purl.org/ontology/bikeo#Brand>
                                                            ] ;
                                            rdfs:comment "is a general class referring to the principal pieces composing a bike."@en ;
                                            rdfs:label "CommonPart"@en .


###  http://purl.org/ontology/bikeo#CommonPartArea
<http://purl.org/ontology/bikeo#CommonPartArea> rdf:type owl:Class ;
                                                rdfs:subClassOf <http://purl.org/ontology/bikeo#CommonPart> ;
                                                rdfs:comment "is a general class referring to all those pieces composed by many smaller parts together."@en ;
                                                rdfs:label "CommonPartArea"@en .


###  http://purl.org/ontology/bikeo#Crank
<http://purl.org/ontology/bikeo#Crank> rdf:type owl:Class ;
                                       rdfs:subClassOf <http://purl.org/ontology/bikeo#CommonPart> ,
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty <http://purl.org/ontology/bikeo#hasChainRing> ;
                                                         owl:someValuesFrom xsd:string
                                                       ] ;
                                       rdfs:comment "is the part of a bicycle that converts the reciprocating motion of the rider's legs into rotational motion used to drive the chain or belt, which in turn drives the rear wheel."@en ;
                                       rdfs:label "Crank"@en .


###  http://purl.org/ontology/bikeo#DownTube
<http://purl.org/ontology/bikeo#DownTube> rdf:type owl:Class ;
                                          rdfs:subClassOf <http://purl.org/ontology/bikeo#CommonPart> ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty <http://purl.org/ontology/bikeo#isComprisedIn> ;
                                                            owl:someValuesFrom <http://purl.org/ontology/bikeo#Frame>
                                                          ] ;
                                          rdfs:comment "the part of frame tube that connects the head tube to the bottom bracket shell."@en ;
                                          rdfs:label "DownTube"@en .


###  http://purl.org/ontology/bikeo#FlatTire
<http://purl.org/ontology/bikeo#FlatTire> rdf:type owl:Class ;
                                          rdfs:subClassOf <http://purl.org/ontology/bikeo#Problem> ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty <http://purl.org/ontology/bikeo#hasSolution> ;
                                                            owl:someValuesFrom <http://purl.org/ontology/bikeo#Inflate>
                                                          ] ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty <http://purl.org/ontology/bikeo#hasSolution> ;
                                                            owl:someValuesFrom <http://purl.org/ontology/bikeo#ReplacePart>
                                                          ] ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty <http://purl.org/ontology/bikeo#isProblemOf> ;
                                                            owl:someValuesFrom <http://purl.org/ontology/bikeo#InnerTube>
                                                          ] ;
                                          rdfs:comment "the process where the tubeless tires lose air."@en ;
                                          rdfs:label "FlatTire"@en .


###  http://purl.org/ontology/bikeo#Fork
<http://purl.org/ontology/bikeo#Fork> rdf:type owl:Class ;
                                      rdfs:subClassOf <http://purl.org/ontology/bikeo#CommonPart> ,
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty <http://purl.org/ontology/bikeo#isComprisedIn> ;
                                                        owl:someValuesFrom <http://purl.org/ontology/bikeo#FrontSet>
                                                      ] ;
                                      rdfs:comment "A bicycle fork is the part of a bicycle that holds the front wheel."@en ;
                                      rdfs:label "Fork"@en .


###  http://purl.org/ontology/bikeo#Frame
<http://purl.org/ontology/bikeo#Frame> rdf:type owl:Class ;
                                       rdfs:subClassOf <http://purl.org/ontology/bikeo#CommonPartArea> ,
                                                       [ rdf:type owl:Restriction ;
